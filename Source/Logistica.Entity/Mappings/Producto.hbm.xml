<?xml version="1.0" encoding="utf-8" ?>
	<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
		<class name="Logistica.Entity.Producto, Logistica.Entity" table="Producto" lazy="false"> 
			<id name="Id" column="ProductoId" type="System.Int64" unsaved-value="0" >
				<generator class="native" />
			</id>
			<property name="Nombre" column="Nombre" type="System.String" length="800" />
			<many-to-one name="TipoProducto" column="TipoProductoId" cascade="none" class="Logistica.Entity.TipoProducto, Logistica.Entity" />
			<many-to-one name="TipoReserva" column="TipoReservaId" cascade="none" class="Logistica.Entity.TipoReserva, Logistica.Entity" />
		</class>
	</hibernate-mapping>
<!--Importante: Asignar el not-null=true que corresponda.-->